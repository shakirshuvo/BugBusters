<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="AliExpress Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="78.0"/>
    <parameter name="url" value="https://www.AliExpress.com"/>

    <test name="AliExpress Home">
        <classes>
            <class name="home.AliExpressHome"></class>
            <class name="Categories.MensFashion"></class>
            <class name="protection.BuysProtectionTest"></class>
            <class name="AliExpressShipToTest.AliExpressShipToTest"></class>
            <class name="AliExpressChangeLanguageTest.TestAliExpressChangeLanguage"></class>
            <class name="AliExpressSearchTest.AliExpressSearchTesting"></class>
        </classes>

    </test>

</suite>