<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Netflix Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="79.0"/>
    <parameter name="url" value="https://www.netflix.com"/>

    <test name="Netflix Home">
        <classes>
<!--            <class name="login.NetflixLoginHassanTest"></class>-->
            <class name="login.NetflixLoginCookieTest"></class>
            <class name="login.NetflixLoginChipsTest"></class>
            <class name="login.NetflixLoginGumTest"></class>
            <class name="login.NetflixLoginHassanTest"></class>
            <class name="login.NetflixLoginChocolateTest"></class>
            <!--            <class name="home.NetflixHomePage"/>-->
            <!--            <class name="home.NetflixHomePOMTest"/>-->
        </classes>
    </test>

</suite>